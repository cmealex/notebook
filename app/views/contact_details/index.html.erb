<h1>Listing contact_details</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Phone1</th>
      <th>Phone2</th>
      <th>Email1</th>
      <th>Email2</th>
      <th>Birthplace</th>
      <th>Residence</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_details.each do |contact_detail| %>
      <tr>
        <td><%= contact_detail.userId %></td>
        <td><%= contact_detail.phone1 %></td>
        <td><%= contact_detail.phone2 %></td>
        <td><%= contact_detail.email1 %></td>
        <td><%= contact_detail.email2 %></td>
        <td><%= contact_detail.birthPlace %></td>
        <td><%= contact_detail.residence %></td>
        <td><%= link_to 'Show', contact_detail %></td>
        <td><%= link_to 'Edit', edit_contact_detail_path(contact_detail) %></td>
        <td><%= link_to 'Destroy', contact_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact detail', new_contact_detail_path %>
